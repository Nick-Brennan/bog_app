<div class="container">
	<div class="jumbotron">
		Name: <%= @creature.name %> <br>
		Description: <%= @creature.description %><br><br>
		<%= link_to "Edit", "/creatures/#{@creature.id}/edit", class: "btn btn-warning btn-sm" %>
		<%= link_to "Back", "/creatures", class: "btn btn-default btn-sm pull-right" %>
		<%= link_to "Delete", "/creatures/#{@creature.id}", method: :delete, class: "btn btn-danger btn-sm pull-right" %>
	</div>
</div>
